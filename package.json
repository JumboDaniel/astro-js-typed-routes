{
  "name": "astro-typed-routes",
  "author": "Jumbo Daniel",
  "version": "0.0.1",
  "type": "module",
  "description": "Type-safe route generation for Astro â€” auto-generated ROUTES object, $path() URL builder, and typed createRoute() helpers.",
  "license": "MIT",
  "keywords": [
    "astro",
    "astro-integration",
    "routes",
    "typed-routes",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/components/link.astro",
    "src/components/link.tsx"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./path": {
      "types": "./dist/path.d.ts",
      "import": "./dist/path.js",
      "require": "./dist/path.cjs"
    },
    "./create-route": {
      "types": "./dist/create-route.d.ts",
      "import": "./dist/create-route.js",
      "require": "./dist/create-route.cjs"
    },
    "./link": "./src/components/link.astro",
    "./link-react": {
      "types": "./dist/link.d.ts",
      "import": "./dist/link.js",
      "require": "./dist/link.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepack": "pnpm build && pnpm lint --fix"
  },
  "peerDependencies": {
    "astro": ">=5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "vite-plugin-dts": "^4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.2",
    "eslint": "^9.39.1",
    "oxlint": "^1.48.0",
    "react": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
